}
.flex-row-r{
  flex-direction: row-reverse
}
.flex-column{
  flex-direction: column
}
.flex-column-r{
  flex-direction: column-reverse
}
/* flex 对齐方式 */
.flex-jc-sb{
  justify-content: space-between
}
/* flex换行  */
.flex-wrap{
  flex-wrap: wrap
}
