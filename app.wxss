.w33{ width: 33.33%;}
.w50{ width: 50%;}
.w100{ width: 100%;}
.h100{height: 100%}
/*边框*/
.btc{ border-top: 1px solid #ccc;}
.bbc{ border-bottom: solid 1px #ccc;}
.brc{ border-right: 1px solid #ccc;}
.blc{ border-left: 1px solid #ccc;}
.br-ec{border-right: 1px solid #EDECEC}
